<div class="container-fluid">
  <blockquote class="blockquote text-center">
    <h2 class="display-5">Start a New Application</h2>
  </<blockquote>
<div class="form-group">
  <%= form_tag "/applications" do %>
  <div class="form-group row">
    <%= label_tag :name%>
    <%= text_field_tag :name %><br>
  </div>
  <div class="form-group row">
    <%= label_tag :street_address %>
    <%= text_field_tag :street_address %><br>
  </div>
  <div class="form-group row">
    <%= label_tag :state%>
    <%= text_field_tag :state %><br>
  </div>
  <div class="form-group row">
    <%= label_tag :city%>
    <%= text_field_tag :city %><br>
  </div>
  <div class="form-group row">
    <%= label_tag :zip_code %>
    <%= text_field_tag :zip_code %><br>
  </div>
    <%= submit_tag 'Create Application', class: "btn btn-primary" %>
  <% end %>
</div>
</div>
