<%= form_tag "/applications" do %>
  <h2>Create a New Application</h2>
  <%= label_tag :name%>
  <%= text_field_tag :name %><br>
  <%= label_tag :street_address %>
  <%= text_field_tag :street_address %><br>
  <%= label_tag :state%>
  <%= text_field_tag :state %><br>
  <%= label_tag :city%>
  <%= text_field_tag :city %><br>
  <%= label_tag :zip_code %>
  <%= text_field_tag :zip_code %><br>
  <%= label_tag :description %>
  <%= text_field_tag :description%><br>
  <%= label_tag :reviewed %>
  <%= text_field_tag :reviewed%><br>
  <%= label_tag :accepted %>
  <%= text_field_tag :accepted%><br>
  <%= submit_tag 'Create Application' %>
<% end %>
